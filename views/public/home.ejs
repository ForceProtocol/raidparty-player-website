<%- partial('./partials/header.ejs') %>

		
<section class="banner-area text-center" id="intro">
	<article class="banner-data">
	
		<% if(flash.has('errors')){ %>
		<br /><br />
		<div class="alert alert-danger col-lg-12 col-md-12 col-sm-12">
			<% flash.get('errors').forEach(function (message) { %>
			<p class="error"><%- message %></p>
			<% }) %>
		</div>
		<% } %>

		<% if(flash.has('success')){ %>
		<br /><br />
		<div class="alert alert-success col-lg-12 col-md-12 col-sm-12">
			<% flash.get('success').forEach(function (message) { %>
			<p class="success"><%- message %></p>
			<% }) %>
		</div>
		<% } %>


    	<div class="banner-logo wow fadeInDown"><img src="images/banner-logo.png" alt="banner-logo"></div>
		
		<br />
		
		<h1 class="wow fadeIn" data-wow-delay="0.1s"><%- __("<span class=\"underline-strong\">50 ETH</span> in prizes on <span class=\"underline-strong\">free to play</span> games!") %></h1>
		<h2 class="wow fadeIn" data-wow-delay="0.1s"><%- __("<strong data-spaces-left-int='%s' class='spaces-left odometer'>%s</strong> spaces left",totalSpaces,totalSpaces.toLocaleString()) %></h2>
		<br />
		<button class="btn btn-cta btn-lg display-join-form"><i class="fa fa-angle-double-down" aria-hidden="true"></i>&nbsp;&nbsp; Enter Free Contest &nbsp;&nbsp;<i class="fa fa-angle-double-down" aria-hidden="true"></i></button>
		
		<div class="container">
			<div class="hide-join-form col-lg-8 col-md-8 col-sm-12 col-xs-12 col-lg-offset-2 col-md-offset-8" style="display:none;">
				<br />
				<form class="form text-left" method="post" action="/join">
					<p><%- __("Fill out the form below to reserve your space.") %></p><br />
					<div class="row form-group">
						<div class="col-lg-6 col-md-6">
							<label>First Name</label>
							<input class="form-control" type="text" name="firstName" value="" />
						</div>
						<div class="col-lg-6 col-md-6">
							<label>Surname</label>
							<input class="form-control" type="text" name="lastName" value="" />
						</div>
					</div>
					
					<div class="form-group">
						<label>Email</label>
						<input class="form-control" type="text" name="email" value="" />
					</div>
					
					<div class="form-group">
						<label>Enter Password</label>
						<input class="form-control" type="password" name="password" value="" />
					</div>
					
					<div class="form-group">
						<label>Re-type Password</label>
						<input class="form-control" type="password" name="passwordCheck" value="" />
					</div>
					
					<%- recaptchaForm %>
					
					<div class="form-group">
						<button class="btn btn-cta btn-block btn-lg">Join Contest &raquo;</button>
					</div>
				</form>
			</div>
		</div>
		
		<!-- SHOW THIS WHEN LIVE ON APP STORES
        <ul class="list-inline app-btns-block">
            <li class="wow fadeInRight"><a href="/"><img src="/images/google-play-btn.png" alt="google-store"></a></li>
        	<li class="wow fadeInLeft"><a href="/"><img src="/images/app-store-btn.png" alt="app-store"></a></li>
        </ul>-->
		
    </article>
	
	<br />
	<br />
	
	
	<article class="banner-data">
		<div class="wow fadeInUp game-list" style="visibility: visible; animation-name: fadeInUp;">
			<div class="container">
			
				<div class="row">
				
					<% games.forEach(function(game){ %>
						<div class="<% if(game.featured){ %>col-md-8<% }else{ %>col-md-4<% } %> col-sm-6">
							<div class="game-holder <% if(game.jackpot.length > 0){ %>has-jackpot<% } %>">
								<div class="game-image minecraft">
									<img src="/images/games/banners/<%- game.bannerContent %>" alt="" />
									<div class="days-left"><%- game.timeLeft %><% if(game.hasActiveCampaign){ %><img src="/images/icons/active-dot.png" alt="active" /><% } %></div>
								</div>
								<div class="jackpot-prize">
									<img src="images/jackpot-icon.svg" alt="jackpot-icon">
									Jackpot Prize : <span><%- game.jackpot %></span>
								</div>
								<div class="game-detail">
									<h1><%- game.title %></h1>
									<ul class="list-inline">
										<% if(game.iosGame){ %>
										<li>
											<a href="<%- game.platform.ios.link %>">PLAY ON IOS</a>
										</li>
										<% } %>
										<% if(game.androidGame){ %>
										<li>
											<a href="<%- game.platform.android.link %>">PLAY ON ANDROID</a>
										</li>
										<% } %>
										<li>
											<a href="#" data-toggle="modal" data-target="#rules">RULES</a>
										</li>
									</ul>
									<div class="clearfix"></div>
								</div>
							</div>
						</div>
					<% }); %>
					
				</div>
			</div>
		</div>
	</article>
	
